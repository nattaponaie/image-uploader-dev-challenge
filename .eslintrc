{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier",
    "prettier/@typescript-eslint"
  ],
  "parserOptions": {
    "ecmaVersion": 9,
    "project": "./tsconfig.json"
  },
  "settings": {
    "import/resolver": {
      "babel-module": {
        "alias": {
          "api": "./src/api",
          "configs": "./src/configs",
          "utils": "./src/utils"
        }
      }
    }
  }
}